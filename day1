2.#include <iostream>
using namespace std;
int main() {
int myNum=5;
double myFloatNum=9.99;
string myText="Hello";
bool myBoolean= true;
cout <<myNum <<endl;
cout <<myFloatNum <<endl;
cout <<myText <<endl;
cout <<myBoolean ;
return 0;
}

3.#include <iostream>
using namespace std;
int main() {
int x=5;
int y =6;
int sum=x+y;
cout <<sum <<endl;
return 0;
}

4.Usage of variables
#include <iostream>
using namespace std;
int main() {
int x,y,z;
x=y=z=50;
int sum=x+y+z;
cout <<sum <<endl;
return 0;
}

5.Constant:
#include <iostream>
using namespace std;
int main() {
const int myNum=15;
int myNum=10;
cout <<sum <<endl;
return 0;
}
/*It will display error message
Const cannot be modified*/

6.#include <iostream>
using namespace std;
int main() {
int x,y;
int sum;
cout <<"Type a number" <<endl;
cin>>x;
cin>>y;
sum=x+y;
cout<<"sum is:"<<sum;
return 0;
}

7.DATA REPRESENTATION
#include <iostream>
using namespace std;
int main() {
int fi=35e3;
double di=12E4;
cout <<fi <<endl;
cout<<di;
return 0;
}
/*Y does the float and double doesnâ€™t have decimal points.to
know about representation and storage. It tells about hardware
and OS*/
Output
35000
120000

8.#include <iostream>
using namespace std;
int main() {
char myGrade='B';
cout<< myGrade;
char a=65;
char b=66;
char c=67;
cout<< a ;
cout<< b ;
cout<< c ;
return 0;
}
Output
