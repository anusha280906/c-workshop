9.#include <iostream>
using namespace std;
int main() {
auto myNum=5;
auto myFloat=9.9;
auto myDoubleNum=8.98;
auto myLetter='D';
auto myBoolean=true ;
auto myString=string("HEllo");
cout<< myNum <<endl;
cout<< myFloat <<endl;
cout<<myDoubleNum <<endl;
cout<< myLetter <<endl ;
cout<<myBoolean ;
return 0;
}
Output
5 9.9
8.98
D1

10.#include <iostream>
#include <string>
using namespace std;
int main() {
string firstName="jo";
string lastName="do";
string fullName= firstName.append(lastName);
cout<< fullName;
return 0;
}
Output
Jodo

11.#include <iostream>
using namespace std;
int main() {
string myString="Hello";
cout<< myString[myString.length()-1];
return 0;
}
Output
o
